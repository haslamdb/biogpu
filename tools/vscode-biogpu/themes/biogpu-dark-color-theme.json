{
    "name": "BioGPU Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#1e1e1e",
        "editor.foreground": "#d4d4d4",
        "editor.lineHighlightBackground": "#2a2a2a",
        "editorCursor.foreground": "#4ECDC4",
        "editorWhitespace.foreground": "#3a3a3a"
    },
    "tokenColors": [
        {
            "scope": ["keyword.control.biogpu", "keyword.declaration.biogpu"],
            "settings": {
                "foreground": "#C678DD"
            }
        },
        {
            "scope": "entity.name.decorator.biogpu",
            "settings": {
                "foreground": "#4ECDC4",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.function.builtin.biogpu",
            "settings": {
                "foreground": "#61AFEF"
            }
        },
        {
            "scope": "string.quoted.biogpu",
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "scope": "constant.numeric.biogpu",
            "settings": {
                "foreground": "#D19A66"
            }
        }
    ]
}
